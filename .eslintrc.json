{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prefer-const" : "error",
        "require-await" : "error",
        "no-console": "off",
        "require-jsdoc": "off"
    }
}
